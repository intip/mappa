  <script type="text/javascript">
  function addcontato()
  {
      var nome = $('#nome').attr('value');
      var email = $('#email').attr('value');
      var ddd1 = $('#ddd1').attr('value');
      var tel1 = $('#tel1').attr('value');
      var tel2 = $('#tel2').attr('value');
      var ddd2 = $('#ddd2').attr('value');


     if (nome == '' || email == 'selecione' || tel1 == '' || ddd1 == '')
      {
        alert('É necessário preencher os campos nome, e-mail e telefone 1.');
      } else {
        var indc = '';
        if ($('#conheceu1').attr('checked'))
            indc += $('#conheceu1').attr('value');
        if ($('#conheceu2').attr('checked'))
            indc += $('#conheceu2').attr('value');
        if ($('#conheceu3').attr('checked'))
            indc += $('#conheceu3').attr('value');
        if ($('#conheceu4').attr('checked'))
        {
            indc += $('#conheceu4').attr('value');
            indc += $('#conheceu44').attr('value') + ', ';
        }
        if ($('#conheceu5').attr('checked'))
        {
            indc += $('#conheceu5').attr('value');
            indc += $('#conheceu55').attr('value');
        }


        $.ajax({
         type: "GET",
         url: "/modulos/form63/addConteudoSite",
         contentType: "application/json; charset=iso-8859-1",
         dataType: "json",
         data: {titulo:nome, id_configuracao:3,
               'bloco_1.campo_1_1:record':nome,
               'bloco_1.campo_1_2:record':email,
               'bloco_1.campo_1_3:record':'(' + ddd1 + ') ' + tel1,
               'bloco_1.campo_1_4:record':'(' + ddd2 + ') ' + tel2,
               'bloco_1.campo_1_5:record': ( $('#newsletter1').attr('checked') ? 'Sim':'Não'),
               'bloco_1.campo_1_6:record':$('#consultor').attr('value'),
               'bloco_1.campo_1_7:record':indc,
               'bloco_1.campo_1_8:record':$('#precisa').attr('value'),
               'bloco_1.campo_1_9:record':( $('#newsletter1').attr('checked') ? 'Sim':'Não'),
               'bloco_1.campo_1_10:record':cod,
			   'bloco_1.campo_1_11:record':$('#pacote').attr('value')
			   },
         success: function(json) {
           if (json['type'] == 'ok')
           {
             alert('Solicitação enviada com sucesso. Obrigado!');
             document.forms['contato'].reset();
           } else
             alert('Ocorreu um erro, tente mais tarde.');
          },
         error: function (XMLHttpRequest, textStatus, errorThrown) {
           alert('Ocorreu um erro, tente mais tarde.');
         }
       });
      }
      return false;
  }
  var cod = undefined;
  $(window).ready(function(){
      $('#pacote').attr('value', 'Informações sobre o pacote: ' + document.referrer);
      var req = new REQUEST();
      cod = req.getItem('cod');
  });
  </script>


